<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="source.MenuController">
   <children>
      <Label text="Inventory Menu">
         <font>
            <Font size="54.0" />
         </font>
      </Label>
      <Button fx:id="buttonCreate" mnemonicParsing="false" onAction="#actionCreate" prefHeight="25.0" prefWidth="629.0" text="Add item to inventory" />
      <TableView fx:id="tableViewBarang" prefHeight="295.0" prefWidth="600.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
   </children>
</VBox>
